<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>additemview</managed-bean-name>
        <managed-bean-class>AddItemView</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>iteminventoryview</managed-bean-name>
        <managed-bean-class>ItemInventoryView</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
        <managed-property>
            <property-name>service</property-name>
            <value>#{itemservice}</value>        
        </managed-property>   
        <managed-property>
            <property-name>categoryDropdown</property-name>
            <value>#{categorydropdown}</value>        
        </managed-property>  
        <managed-property>
            <property-name>shoppingCart</property-name>
            <value>#{shoppingcart}</value>        
        </managed-property>                     
    </managed-bean> 
     <managed-bean>
        <managed-bean-name>itemservice</managed-bean-name>
        <managed-bean-class>ItemService</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>      
    <managed-bean>
        <managed-bean-name>categorydropdown</managed-bean-name>
        <managed-bean-class>CategoryDropdown</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>discountdropdown</managed-bean-name>
        <managed-bean-class>DiscountDropdown</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>shoppingcart</managed-bean-name>
        <managed-bean-class>ShoppingCart</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>    
    <managed-bean>
        <managed-bean-name>itemimage</managed-bean-name>
        <managed-bean-class>ItemImage</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>iteminventory</managed-bean-name>
        <managed-bean-class>ItemInventory</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>selector</managed-bean-name>
        <managed-bean-class>Selector</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>util</managed-bean-name>
        <managed-bean-class>Util</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>customer</managed-bean-name>
        <managed-bean-class>Customer</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>login</property-name>
            <value>#{login}</value>
        </managed-property>
    </managed-bean>    
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>  
    <managed-bean>
        <managed-bean-name>customerInfo</managed-bean-name>
        <managed-bean-class>CustomerInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cart</managed-bean-name>
        <managed-bean-class>Cart</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>  
    <managed-bean>
        <managed-bean-name>admin</managed-bean-name>
        <managed-bean-class>Admin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>employee</managed-bean-name>
        <managed-bean-class>Employee</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>discountPage</managed-bean-name>
        <managed-bean-class>DiscountPage</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>categoryPage</managed-bean-name>
        <managed-bean-class>CategoryPage</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>orderHistory</managed-bean-name>
        <managed-bean-class>OrderHistory</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean> 
    <navigation-rule>
        <from-view-id>
            /index.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/itemInventory.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/admin.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>discounts</from-outcome>
            <to-view-id>/discounts.xhtml</to-view-id>
           
        </navigation-case>
        <navigation-case>
            <from-outcome>categories</from-outcome>
            <to-view-id>/category.xhtml</to-view-id>
            
        </navigation-case>
    </navigation-rule> 
    
    <navigation-rule>
        <from-view-id>
            /start.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>newCustomer</from-outcome>
            <to-view-id>/newCustomer.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>listCustomers</from-outcome>
            <to-view-id>/showAllCustomers.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>findCustomer</from-outcome>
            <to-view-id>/findCustomer.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteCustomer</from-outcome>
            <to-view-id>/deleteCustomer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /signup.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /start.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /newCustomer.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/start.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /findCustomer.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>showCustomer</from-outcome>
            <to-view-id>/displayCustomer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /deleteCustomer.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/start.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
